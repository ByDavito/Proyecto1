<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>@ViewData["Title"] - Proyecto1</title>
  <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
  <link rel="stylesheet" href="~/lib/fontawesome/css/fontawesome.min.css">
  <link rel="stylesheet" href="~/lib/fontawesome/css/brands.min.css">
  <link rel="stylesheet" href="~/lib/fontawesome/css/solid.min.css">
  <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
  <link rel="stylesheet" href="~/Proyecto1.styles.css" asp-append-version="true" />
  <link rel="stylesheet" href="~/css/style-login.css" asp-append-version="true">
</head>

<body>


  <nav class="navbar fixed-top">
    <div class="container-fluid">
      <a class="d-flex align-items-center justify-content-start" >
        <div class="logo" asp-controller="Home" asp-action="Index"></div>
        <div class="logo-text" href=""></div>
      </a>
         <div class="background">
        <button class="menu__icon" type="button" data-bs-toggle="offcanvas" data-bs-target="#offcanvasNavbar" id="botonMenu" aria-controls="offcanvasNavbar" onclick="animacionMenu()">
          <span></span>
          <span></span>
          <span></span>
        </button>
      </div>
    </div>
  </nav>




  <div class="offcanvas offcanvas-end mi-offcanvas" tabindex="-1" id="offcanvasNavbar"
    aria-labelledby="offcanvasNavbarLabel">
    <div class="offcanvas-header">
    </div>
    <div class="offcanvas-body">
      <ul class="navbar-nav justify-content-end flex-grow-1 pe-3">
        <li class="nav-item">
          <a class="nav-link text-light" asp-area="" asp-controller="EjerciciosFisicos" asp-action="Graficos"><i class="fa-solid fa-chart-column nav-iconos"></i>  Graficos</a>
          <span></span>
        </li>
        
        <li class="nav-item">
          <a class="nav-link text-light" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
           <span></span>
        </li>

        <li class="nav-item">
          <a class="nav-link text-light" asp-area="" asp-controller="Ejercicios" asp-action="Ejercicios"> <i class="fa-solid fa-dumbbell nav-iconos"></i>
           Tipo de Ejercicios</a>
          <span></span>
        </li>

        <li class="nav-item">
          <a class="nav-link text-light" asp-area="" asp-controller="Lugares" asp-action="Lugares"> <i class="fa-solid fa-dumbbell nav-iconos"></i>
           Lugares</a>
          <span></span>
        </li>

        <li class="nav-item">
          <a class="nav-link text-light" asp-area="" asp-controller="EjerciciosFisicos" asp-action="Index"><i class="fa-solid fa-person-biking nav-iconos"></i>
             Ejercicios</a>
            <span></span>
        </li>

        <li class="nav-item">
          <a class="nav-link text-light" asp-area="" asp-controller="EjerciciosFisicos" asp-action="Informe"><i class="fa-solid fa-person-biking nav-iconos"></i>
             Informe</a>
            <span></span>
        </li>

          <partial name="_LoginPartial" /> 
            <span></span> 
      </ul>

    </div>
  </div>
  

  <div class="container centrar">
    <main role="main" class="pb-3">
      @RenderBody()
    </main>
  </div>

 
 
  

  <footer class="footer text-muted">
    <div class="container">
      &copy; 2024 - FitLogg - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
    </div>
  </footer>
  <script src="~/lib/jquery/dist/jquery.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
  <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
  <script src="~/js/site.js" asp-append-version="true"></script>
  @await RenderSectionAsync("Scripts", required: false)
</body>

</html>
